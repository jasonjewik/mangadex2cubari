{
  "name": "MangaDex2Cubari",
  "description": "Redirect MangaDex/Imgur URLs to their Cubari complement.",
  "version": "1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus", "tabs"],
  "host_permissions": ["*://*.mangadex.org/*", "*://*.cubari.moe/*"]
}
